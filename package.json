{
  "name": "thilo-frontend",
  "homepage": "https://scout-ch.github.io/thilo",
  "description": "Thilo Frontend App",
  "author": "Bodo Braegger",
  "developer": {
    "name": "Thilo App Team"
  },
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "release": "npm run lint && npm run format:check && npm run ts-check && npm run spellcheck && npm run test && npm run build",
    "build": "npm run build:production",
    "build:production": "npm run clean:dist && webpack --node-env production",
    "build:development": "npm run clean:dist && webpack --node-env development",
    "bundle-analysis": "webpack --node-env production --env showBundleAnalysis",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --runInBand",
    "ts-check": "tsc",
    "format": "prettier . --write --cache --loglevel warn",
    "format:check": "prettier . --check --cache --loglevel warn",
    "spellcheck": "cspell --no-progress --dot \"**/*.{js,ts,jsx,tsx,json,html,xml,svg,css,scss,sass,md}\"",
    "list-outdated-dependencies": "npm-check-updates --format repo,group --peer",
    "update-dependencies": "npm run list-outdated-dependencies -- -u && npm install && npm update && npm dedupe && npm run lint:fix && npm run format && npm run release",
    "clean": "rimraf node_modules coverage dist",
    "clean:dist": "rimraf dist"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.2.6",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.1.4",
    "react-markdown": "^8.0.5",
    "react-quiz-component": "^0.5.1",
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0",
    "remark-gfm": "^3.0.1",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.31.0",
    "cspell": "^6.31.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.43.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-minimizer-webpack-plugin": "^4.4.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "license-webpack-plugin": "^4.0.2",
    "lint-staged": "^13.2.3",
    "mini-css-extract-plugin": "^2.7.6",
    "mini-svg-data-uri": "^1.4.4",
    "npm-check-updates": "^16.10.13",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^8.5.1",
    "prettier": "^2.8.8",
    "react-error-overlay": "6.0.9",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}
